<div class="row">
  {{#each indicators}}
    <div class="grid-2 grid-md-10 grid-sm-10">
      <article class="mod-indicators-item" data-type="{{type}}">
        <header class="mod-indicators-header">
          <a href="#" class="mod-indicators-agency" data-agency="{{agency}}">{{agency}}</a>
          <h2>{{name}}</h2>
        </header>
        <div class="mod-indicators-value">
          <strong class="mod-indicators-value-item is-hidden" data-period="year">{{yearly}}%</strong>
          <strong class="mod-indicators-value-item is-hidden" data-period="month">{{monthly}}%</strong>
          <strong class="mod-indicators-value-item is-hidden" data-period="week">{{weekly}}%</strong>
          <strong class="mod-indicators-value-item is-hidden" data-period="day">{{daily}}%</strong>
        </div>
        <div class="mod-indicators-footer sm-hidden">
          <table>
            <tr>
              <th>FYTD</th>
              <th>Previous FYTD</th>
            </tr>
            <tr>
              <td>{{year}}%</td>
              <td>{{yearAgo}}%</td>
            </tr>
          </table>
        </div>
      </article>
    </div>
  {{/each}}
</div>
