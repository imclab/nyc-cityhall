<div class="mod-infowindow">
  <a href="#close" class="icon icon-infowindow-close close"></a>
  <header class="mod-infowindow-header">
    <h2>{{name}}</h2>
  </header>
  <div class="mod-infowindow-content">
    <table>
      {{#previous}}
        <td>
          <strong>Current</strong><br>
          {{#current}}{{current}}{{/current}}
          {{^current}}0{{/current}}
        </td>
        <td><strong>Previous</strong><br>{{previous}}</td>
      {{/previous}}
      <td class="mod-infowindow-value">
        {{#last_monthdayyear}}
          {{last_monthdayyear}}%
        {{/last_monthdayyear}}
        {{^last_monthdayyear}}
          {{#current}}{{current}}{{/current}}
          {{^current}}0{{/current}}
        {{/last_monthdayyear}}
      </td>
    </table>
  </div>
</div>
