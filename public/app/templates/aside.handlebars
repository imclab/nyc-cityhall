<div class="mod-aside-content">
  <button class="icon icon-close"></button>
  <ul class="mod-aside-agencies">
    <li><a href="#all" class="current" data-agency="all">All agencies</a></li>
    {{#each agencies}}
      <li><a href="#{{name}}" data-agency="{{name}}">{{name}}</a></li>
    {{/each}}
  </ul>

  <ul class="mod-aside-types">
    <li><a href="#all" class="current" data-type="all">All indicators</a></li>
    {{#each types}}
      <li><a href="#{{name}}" data-type="{{name}}">{{name}}</a></li>
    {{/each}}
  </ul>
  <ul class="mod-aside-priorities">
    <li><a href="#urgent"  data-priority="urgent">Urgent</a></li>
    <li><a href="#worsening" data-priority="worsening">Worsening</a></li>
    <li><a href="#improving" data-priority="improving">Improving</a></li>
  </ul>
</div>
